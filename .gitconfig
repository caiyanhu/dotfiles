[user]
	name = caiyanhu
	email = caiyanhu@cmss.chinamobile.com
[http "https://github.com"]
	proxy = socks5://127.0.0.1:7890
[commit]
	template = /Users/caiyanhu/.git_commit_template
[core]
	editor = nvim
[diff]
  tool = nvimdiff
[difftool]
  prompt = false
[difftool "nvimdiff"]
  cmd = "nvim -d \"$LOCAL\" \"$REMOTE\""
[merge]
  tool = nvimdiff
[mergetool]
  prompt = true
[mergetool "nvimdiff"]
  cmd = "nvim -d \"$LOCAL\" \"$REMOTE\" \"$MERGED\" -c 'wincmd w' -c 'wincmd J'"
